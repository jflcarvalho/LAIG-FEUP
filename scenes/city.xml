<MATERIAL id="m_white_metal">
  <shininess value="100" />
  <specular r="0.8" g="0.8" b="0.8" a="1.0" />
  <diffuse r="0.5" g="0.5" b="0.5" a="1.0" />
  <ambient r="0.0" g="0.0" b="0.0" a="1.0" />
  <emission r="0.0" g="0.0" b="0.0" a="1.0" />
</MATERIAL>

<NODE id="city">
  <MATERIAL id="null"/>
  <TEXTURE  id="clear"/>
  <DESCENDANTS>
    <NODEREF id="dome"/>
  <DESCENDANTS/>
<NODE/>

<NODE id="dome">
  <MATERIAL id="m_white_metal"/>
  <TEXTURE  id="clear"/>
  <DESCENDANTS>
    <NODEREF id="dome_sphere"/>
    <NODEREF id="dome_cylinder"/>
  <DESCENDANTS/>
<NODE/>

<NODE id="dome_sphere">
  <MATERIAL id="null"/>
  <TEXTURE  id="clear"/>
  <LEAF type="sphere" args="1.0 10 10"/>
<NODE/>

<NODE id="dome_cylinder">
  <MATERIAL id="null"/>
  <TEXTURE  id="clear"/>
  <LEAF type="cylinder" args="0.5 1.0 1.0 10 10"/>
<NODE/>

<NODE id="green_house">
  <MATERIAL id="glass"/>
  <TEXTURE  id="clear"/>
  <LEAF type="cylinder" args="10 1.0 1.0 10 10"/>
<NODE/>
