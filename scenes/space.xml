<SCENE>

  <INITIALS>
    <frustum near="0.1" far="500"/>
    <translation x="0" y="0" z="0" />
    <rotation axis="x" angle="0" />
    <rotation axis="y" angle="0" />
    <rotation axis="z" angle="0" />
    <scale sx="0.2" sy="0.2" sz="0.2" />
    <reference length="1" />
  </INITIALS>


  <ILLUMINATION>
    <ambient r="0.25" g="0.25" b="0.25" a="1" />
    <background r="0" g="0" b="0" a="1" />
  </ILLUMINATION>


  <LIGHTS>
    <LIGHT id="l_sun">
      <enable value ="1" />
      <position  x="0" y="0" z="0" w="1" />
      <ambient r="0.1" g="0.1" b="0.1" a="1.0" />
      <diffuse r="1.0" g="1.0" b="1.0" a="1.0" />
      <specular r="1.0" g="1.0" b="1.0" a="1.0" />
    </LIGHT>
  </LIGHTS>


  <TEXTURES>
    <TEXTURE id="t_sun">
      <file path="images/solar_system/sun.jpg" />
      <amplif_factor s="1" t="1" />
    </TEXTURE>

    <TEXTURE id="t_mercury">
      <file path="images/solar_system/mercury.jpg" />
      <amplif_factor s="1" t="1" />
    </TEXTURE>

    <TEXTURE id="t_venus">
      <file path="images/solar_system/venus.jpg" />
      <amplif_factor s="1" t="1" />
    </TEXTURE>

    <TEXTURE id="t_earth">
      <file path="images/solar_system/earth.jpg" />
      <amplif_factor s="1" t="1" />
    </TEXTURE>
    
    <TEXTURE id="t_moon">
      <file path="images/solar_system/moon.jpg" />
      <amplif_factor s="1" t="1" />
    </TEXTURE>

    <TEXTURE id="t_mars">
      <file path="images/solar_system/mars.jpg" />
      <amplif_factor s="1" t="1" />
    </TEXTURE>

    <TEXTURE id="t_jupiter">
      <file path="images/solar_system/jupiter.jpg" />
      <amplif_factor s="1" t="1" />
    </TEXTURE>

    <TEXTURE id="t_saturn">
      <file path="images/solar_system/saturn.jpg" />
      <amplif_factor s="1" t="1" />
    </TEXTURE>

    <TEXTURE id="t_saturn_ring">
      <file path="images/solar_system/saturn_ring.png" />
      <amplif_factor s="1" t="1" />
    </TEXTURE>

    <TEXTURE id="t_uranus">
      <file path="images/solar_system/uranus.jpg" />
      <amplif_factor s="1" t="1" />
    </TEXTURE>

    <TEXTURE id="t_uranus_ring">
      <file path="images/solar_system/uranus_ring.png" />
      <amplif_factor s="1" t="1" />
    </TEXTURE>

    <TEXTURE id="t_neptune">
      <file path="images/solar_system/neptune.jpg" />
      <amplif_factor s="1" t="1" />
    </TEXTURE>

    <TEXTURE id="t_rubber">
      <file path="images/misc/tyre_rubber.jpg" />
      <amplif_factor s="1" t="1" />
    </TEXTURE>

    <TEXTURE id="t_aluminium">
      <file path="images/misc/aluminium.jpg" />
      <amplif_factor s="2" t="1" />
    </TEXTURE>

    <TEXTURE id="t_skybox_back">
      <file path="images/solar_system/skybox_back.png" />
      <amplif_factor s="1" t="1" />
    </TEXTURE>

    <TEXTURE id="t_skybox_down">
      <file path="images/solar_system/skybox_down.png" />
      <amplif_factor s="1" t="1" />
    </TEXTURE>

    <TEXTURE id="t_skybox_front">
      <file path="images/solar_system/skybox_front.png" />
      <amplif_factor s="1" t="1" />
    </TEXTURE>

    <TEXTURE id="t_skybox_left">
      <file path="images/solar_system/skybox_left.png" />
      <amplif_factor s="1" t="1" />
    </TEXTURE>

    <TEXTURE id="t_skybox_right">
      <file path="images/solar_system/skybox_right.png" />
      <amplif_factor s="1" t="1" />
    </TEXTURE>

    <TEXTURE id="t_skybox_up">
      <file path="images/solar_system/skybox_up.png" />
      <amplif_factor s="1" t="1" />
    </TEXTURE>

    <TEXTURE id="t_pyramid">
      <file path="images/misc/rock.jpg" />
      <amplif_factor s="0.5" t="0.5" />
    </TEXTURE>

    <TEXTURE id="t_perfurated">
      <file path="images/misc/perforated_metal.jpg" />
      <amplif_factor s="1" t="1" />
    </TEXTURE>

    <TEXTURE id="t_carbon">
      <file path="images/rocket/carbon.jpg" />
      <amplif_factor s="1" t="1" />
    </TEXTURE>

    <TEXTURE id="t_dark_grill">
      <file path="images/rocket/dark_grill.jpg" />
      <amplif_factor s="1" t="1" />
    </TEXTURE>

    <TEXTURE id="t_light_grill">
      <file path="images/rocket/light_grill.jpg" />
      <amplif_factor s="1" t="1" />
    </TEXTURE>
    
    <TEXTURE id="cone_t">
      <file path="images/rocket/cone.jpg" />
      <amplif_factor s="1" t="1" />
    </TEXTURE>
  </TEXTURES>


  <MATERIALS>
    <MATERIAL id="m_default">
      <shininess value="10" />
      <specular r="0.5" g="0.5" b="0.5" a="1" />
      <diffuse r="0.5" g="0.5" b="0.5" a="1" />
      <ambient r="0.1" g="0.1" b="0.1" a="1" />
      <emission r="0.0" g="0.0" b="0.0" a="1" />
    </MATERIAL>

    <MATERIAL id="m_planets">
      <shininess value="4" />
      <specular r="0.25" g="0.25" b="0.25" a="1" />
      <diffuse r="0.75" g="0.75" b="0.75" a="1" />
      <ambient r="0.1" g="0.1" b="0.1" a="1" />
      <emission r="0.0" g="0.0" b="0.0" a="1" />
    </MATERIAL>

    <MATERIAL id="m_sun">
      <shininess value="1" />
      <specular r="0.0" g="0.0" b="0.0" a="1" />
      <diffuse r="0.0" g="0.0" b="0.0" a="1" />
      <ambient r="0.0" g="0.0" b="0.0" a="1" />
      <emission r="1.0" g="0.5" b="0.0" a="1" />
    </MATERIAL>

    <MATERIAL id="m_robot_steel">
      <shininess value="20" />
      <specular r="0.85" g="0.85" b="0.85" a="1" />
      <diffuse r="0.5" g="0.5" b="0.5" a="1" />
      <ambient r="0.1" g="0.1" b="0.1" a="1" />
      <emission r="0" g="0" b="0" a="1" />
    </MATERIAL>

    <MATERIAL id="m_robot_wheel">
      <shininess value="1" />
      <specular r="0.2" g="0.2" b="0.2" a="1" />
      <diffuse r="0.6" g="0.6" b="0.6" a="1" />
      <ambient r="0.1" g="0.1" b="0.1" a="1" />
      <emission r="0" g="0" b="0" a="1" />
    </MATERIAL>

    <MATERIAL id="m_pyramid">
      <shininess value="1" />
      <specular r="0.1" g="0.1" b="0.1" a="1" />
      <diffuse r="0.5" g="0.5" b="0.5" a="1" />
      <ambient r="0.1" g="0.1" b="0.1" a="1" />
      <emission r="0.0" g="0.0" b="0.0" a="1" />
    </MATERIAL>

    <MATERIAL id="m_skybox">
      <shininess value="1" />
      <specular r="0.0" g="0.0" b="0.0" a="1" />
      <diffuse r="1.0" g="1.0" b="1.0" a="1" />
      <ambient r="1.0" g="1.0" b="1.0" a="1" />
      <emission r="0.0" g="0.0" b="0.0" a="1" />
    </MATERIAL>

    <MATERIAL id="m_spaceship_body">
      <shininess value="64" />
      <specular r="1.0" g="1.0" b="1.0" a="1" />
      <diffuse r="1.0" g="1.0" b="1.0" a="1" />
      <ambient r="0.1" g="0.1" b="0.1" a="1" />
      <emission r="0.0" g="0.0" b="0.0" a="1" />
    </MATERIAL>

    <MATERIAL id="m_spaceship_nozzle">
      <shininess value="16" />
      <specular r="1.0" g="1.0" b="1.0" a="1" />
      <diffuse r="0.75" g="0.75" b="0.75" a="1" />
      <ambient r="0.1" g="0.1" b="0.1" a="1" />
      <emission r="0.0" g="0.0" b="0.0" a="1" />
    </MATERIAL>

    <MATERIAL id="m_rocket_nose">
      <shininess value="25" />
      <specular r="0.8" g="0.8" b="0.8" a="1" />
      <diffuse r="0.5" g="0.5" b="0.5" a="1" />
      <ambient r="0.1" g="0.1" b="0.1" a="1" />
      <emission r="0.0" g="0.0" b="0.0" a="1" />
    </MATERIAL>

    <MATERIAL id="m_rocket_union">
      <shininess value="20" />
      <specular r="0.2" g="0.3" b="0.7" a="1" />
      <diffuse r="0.2" g="0.3" b="0.7" a="1" />
      <ambient r="0.1" g="0.1" b="0.2" a="1" />
      <emission r="0.0" g="0.0" b="0.0" a="1" />
    </MATERIAL>

    <MATERIAL id="m_rocket_stage_body">
      <shininess value="25" />
      <specular r="0.8" g="0.8" b="0.8" a="1" />
      <diffuse r="0.5" g="0.5" b="0.5" a="1" />
      <ambient r="0.1" g="0.1" b="0.1" a="1" />
      <emission r="0.0" g="0.0" b="0.0" a="1" />
    </MATERIAL>

    <MATERIAL id="m_rocket_nozzle">
      <shininess value="1" />
      <specular r="0.1" g="0.1" b="0.1" a="1" />
      <diffuse r="0.8" g="0.8" b="0.8" a="1" />
      <ambient r="0.1" g="0.1" b="0.1" a="1" />
      <emission r="0.0" g="0.0" b="0.0" a="1" />
    </MATERIAL>
  </MATERIALS>

  <ANIMATIONS>
    <ANIMATION id="an_1" speed="1.6" type="linear">
      <controlpoint xx="1.4" yy="5.3" zz="1.2" />
      <controlpoint xx="-1" yy="-5.0" zz="-3" />
    </ANIMATION>
    <ANIMATION id="an_mercury" speed="300" type="circular" 
              centerx="0" centery="0" centerz="0" 
              radius="50" startang="180" rotang="3600"/>
    <ANIMATION id="an_venus" speed="300" type="circular" 
              centerx="0" centery="0" centerz="0" 
              radius="65" startang="180" rotang="3600"/>
    <ANIMATION id="an_earth" speed="300" type="circular" 
              centerx="0" centery="0" centerz="0" 
              radius="80" startang="90" rotang="3600"/>
    <ANIMATION id="an_moon" speed="300" type="circular" 
              centerx="0" centery="0" centerz="0" 
              radius="5" startang="180" rotang="3600"/>
    <ANIMATION id="an_mars" speed="300" type="circular" 
              centerx="0" centery="0" centerz="0" 
              radius="95" startang="90" rotang="3600"/>
    <ANIMATION id="an_jupiter" speed="300" type="circular" 
              centerx="0" centery="0" centerz="0" 
              radius="145" startang="90" rotang="3600"/>
    <ANIMATION id="an_saturn" speed="300" type="circular" 
              centerx="0" centery="0" centerz="0" 
              radius="205" startang="90" rotang="3600"/>
    <ANIMATION id="an_uranus" speed="300" type="circular" 
              centerx="0" centery="0" centerz="0" 
              radius="255" startang="90" rotang="3600"/>
    <ANIMATION id="an_neptune" speed="300" type="circular" 
              centerx="0" centery="0" centerz="0" 
              radius="295" startang="90" rotang="3600"/>
    <ANIMATION id="an_3" speed="10" type="bezier">
      <controlpoint xx="1" yy="2" zz="3" /> <!-- P1 -->
      <controlpoint xx="1" yy="2" zz="4" /> <!-- P2 -->
      <controlpoint xx="1" yy="2" zz="5" /> <!-- P3 -->
      <controlpoint xx="1" yy="2" zz="6" /> <!-- P4 -->
    </ANIMATION>
    <ANIMATION id="an_4" type="combo"> 
    <!-- pode ser repetida para
    várias animações.
    a próxima instrução deve existir pelo menos uma vez
    com id a referir-se a uma animação definida acima.
    As animacoes devem ser executadas sequencialmente
    pela ordem com que sao declaradas -->
      <SPANREF id="an_1" />
    </ANIMATION>
  </ANIMATIONS>

  <NODES>
    <ROOT id="root" />

    <NODE id="root">
      <MATERIAL id="m_default" />
      <TEXTURE id="clear" />
      <DESCENDANTS>
        <NODEREF id="skybox" />
        <NODEREF id="solar_system" />
      </DESCENDANTS>
    </NODE>

    <!-- Solar System -->
    <NODE id="solar_system">
      <MATERIAL id="null" />
      <TEXTURE id="null" />
      <DESCENDANTS>
        <NODEREF id="sun" />
        <NODEREF id="planets" />
      </DESCENDANTS>
    </NODE>

    <!-- Sun -->
    <NODE id="sun">
      <MATERIAL id="m_sun" />
      <TEXTURE id="t_sun" />
      <ROTATION axis="x" angle="-90" />
      <DESCENDANTS>
        <LEAF id="p_sun" type="sphere" args="20 40 40" />
      </DESCENDANTS>
    </NODE>

    <!-- Planets -->
    <NODE id="planets">
      <MATERIAL id="m_planets" />
      <TEXTURE id="null" />
      <DESCENDANTS>
        <NODEREF id="mercury" />
        <NODEREF id="venus" />
        <NODEREF id="earth_system" />
        <NODEREF id="mars" />
        <NODEREF id="jupiter" />
        <NODEREF id="saturn" />
        <NODEREF id="uranus" />
        <NODEREF id="neptune" />
      </DESCENDANTS>
    </NODE>

    <NODE id="mercury">
      <MATERIAL id="null" />
      <TEXTURE id="t_mercury" />
      <ROTATION axis="x" angle="-90" />
      <ANIMATIONREFS>
        <ANIMATIONREF id="an_mercury" />
      </ANIMATIONREFS>
      <DESCENDANTS>
        <LEAF id="p_mercury" type="sphere" args="2 20 20" />
      </DESCENDANTS>
    </NODE>

    <NODE id="venus">
      <MATERIAL id="null" />
      <TEXTURE id="t_venus" />
      <ROTATION axis="x" angle="-90" />
      <ANIMATIONREFS>
        <ANIMATIONREF id="an_venus" />
      </ANIMATIONREFS>
      <DESCENDANTS>
        <NODEREF id="rocket" />
        <LEAF id="p_venus" type="sphere" args="3 24 24" />
      </DESCENDANTS>
    </NODE>

    <NODE id="earth_system">
      <MATERIAL id="null" />
      <TEXTURE id="t_earth" />
      <ANIMATIONREFS>
        <ANIMATIONREF id="an_earth" />
      </ANIMATIONREFS>
      <DESCENDANTS>
        <NODEREF id="moon" />
        <NODEREF id="earth" />
      </DESCENDANTS>
    </NODE>

    <NODE id="earth">
      <MATERIAL id="null" />
      <TEXTURE id="t_earth" />
      <ROTATION axis="x" angle="-90" />
      <DESCENDANTS>
        <NODEREF id="pyramid" />
        <LEAF id="p_earth" type="sphere" args="3 24 24" />
      </DESCENDANTS>
    </NODE>

    <NODE id="moon">
      <MATERIAL id="null" />
      <TEXTURE id="t_moon" />
      <ROTATION axis="y" angle="-90" />
      <ANIMATIONREFS>
        <ANIMATIONREF id="an_moon" />
      </ANIMATIONREFS>
      <DESCENDANTS>
        <LEAF id="p_moon" type="sphere" args="1 18 18" />
      </DESCENDANTS>
    </NODE>

    <NODE id="mars">
      <MATERIAL id="null" />
      <TEXTURE id="t_mars" />
      <ROTATION axis="x" angle="-90" />
      <ANIMATIONREFS>
        <ANIMATIONREF id="an_mars" />
      </ANIMATIONREFS>
      <DESCENDANTS>
        <NODEREF id="robot" />
        <LEAF id="p_mars" type="sphere" args="2 20 20" />
      </DESCENDANTS>
    </NODE>

    <NODE id="jupiter">
      <MATERIAL id="null" />
      <TEXTURE id="t_jupiter" />
      <ROTATION axis="x" angle="-90" />
      <ANIMATIONREFS>
        <ANIMATIONREF id="an_jupiter" />
      </ANIMATIONREFS>
      <DESCENDANTS>
        <LEAF id="p_jupiter" type="sphere" args="10 30 30" />
      </DESCENDANTS>
    </NODE>

    <NODE id="saturn">
      <MATERIAL id="null" />
      <TEXTURE id="t_saturn" />
      <ROTATION axis="x" angle="-90" />
      <ANIMATIONREFS>
        <ANIMATIONREF id="an_saturn" />
      </ANIMATIONREFS>
      <DESCENDANTS>
        <LEAF id="p_saturn" type="sphere" args="7.5 28 28" />
        <NODEREF id="saturn_ring" />
      </DESCENDANTS>
    </NODE>

    <NODE id="saturn_ring">
      <MATERIAL id="null" />
      <TEXTURE id="t_saturn_ring" />
      <SCALE sx="17.5" sy="17.5" sz="17.5" />
      <ROTATION axis="y" angle="30" />
      <DESCENDANTS>
        <LEAF id="p_saturn_ring" type="cylinder" args="0 1 1 0 28 1 1" />
      </DESCENDANTS>
    </NODE>

    <NODE id="uranus">
      <MATERIAL id="null" />
      <TEXTURE id="t_uranus" />
      <ROTATION axis="x" angle="-90" />
      <ANIMATIONREFS>
        <ANIMATIONREF id="an_uranus" />
      </ANIMATIONREFS>
      <DESCENDANTS>
        <LEAF id="p_uranus" type="sphere" args="5 26 26" />
        <NODEREF id="uranus_ring" />
      </DESCENDANTS>
    </NODE>

    <NODE id="uranus_ring">
      <MATERIAL id="null" />
      <TEXTURE id="t_uranus_ring" />
      <SCALE sx="15" sy="15" sz="15" />
      <ROTATION axis="y" angle="15" />
      <DESCENDANTS>
        <LEAF id="p_uranus_ring" type="cylinder" args="0 1 1 0 32 1 1" />
      </DESCENDANTS>
    </NODE>

    <NODE id="neptune">
      <MATERIAL id="null" />
      <TEXTURE id="t_neptune" />
      <ROTATION axis="x" angle="-90" />
      <ANIMATIONREFS>
        <ANIMATIONREF id="an_neptune" />
      </ANIMATIONREFS>
      <DESCENDANTS>
        <LEAF id="t_neptune" type="sphere" args="5 26 26" />
      </DESCENDANTS>
    </NODE>
    
    <!-- Pyramid -->
    <NODE id="pyramid">
      <MATERIAL id="m_pyramid" />
      <TEXTURE id="t_pyramid" />
      <TRANSLATION x="0" y="0" z="3.2" />
      <ROTATION axis="x" angle="90" />
      <DESCENDANTS>
        <NODEREF id="pyramid_faces" />
        <NODEREF id="pyramid_base" />
      </DESCENDANTS>
    </NODE>

    <NODE id="pyramid_faces">
      <MATERIAL id="null" />
      <TEXTURE id="null" />
      <DESCENDANTS>
        <LEAF id="face_1" type="triangle" args="-0.5 0 0.5 0.5 0 0.5 0 0.5 0" />
        <LEAF id="face_2" type="triangle" args="0.5 0 0.5 0.5 0 -0.5 0 0.5 0" />
        <LEAF id="face_3" type="triangle" args="0.5 0 -0.5 -0.5 0 -0.5 0 0.5 0" />
        <LEAF id="face_4" type="triangle" args="-0.5 0 -0.5 -0.5 0 0.5 0 0.5 0" />
      </DESCENDANTS>
    </NODE>

    <NODE id="pyramid_base">
      <MATERIAL id="null" />
      <TEXTURE id="null" />
      <ROTATION axis="x" angle="90" />
      <DESCENDANTS>
        <LEAF id="base" type="rectangle" args="-0.5 0.5 0.5 -0.5" />
      </DESCENDANTS>
    </NODE>

    <!-- Robot -->
    <NODE id="robot">
      <MATERIAL id="m_robot_steel" />
      <TEXTURE id="t_aluminium" />
      <TRANSLATION x="0" y="0" z="2.2" />
      <SCALE sx="0.5" sy="0.5" sz="0.5" />
      <ROTATION axis="x" angle="90" />
      <DESCENDANTS>
        <NODEREF id="body" />
        <NODEREF id="camera" />
        <NODEREF id="wheels" />
      </DESCENDANTS>
    </NODE>

    <NODE id="camera">
      <MATERIAL id="null" />
      <TEXTURE id="null" />

      <ROTATION axis="x" angle="-90" />

      <DESCENDANTS>
        <NODEREF id="camera1" />
        <NODEREF id="camera2" />
        <LEAF id="p_cyl1" type="cylinder" args="1 0.05 0.05 20 20 1 1" />
      </DESCENDANTS>
    </NODE>

    <NODE id="camera1">
      <MATERIAL id="null" />
      <TEXTURE id="null" />

      <TRANSLATION x="0" y="0" z="1" />
      <SCALE sx="0.1" sy="0.4" sz="0.25" />

      <DESCENDANTS>
        <NODEREF id="box" />
      </DESCENDANTS>
    </NODE>

    <NODE id="camera2">
      <MATERIAL id="null" />
      <TEXTURE id="null" />

      <TRANSLATION x="0" y="0" z="1" />
      <ROTATION axis="y" angle="90" />

      <DESCENDANTS>
        <LEAF id="p_cyl1" type="cylinder" args="0.1 0.05 0.05 20 20 1 1" />
      </DESCENDANTS>
    </NODE>

    <NODE id="body">
      <MATERIAL id="null" />
      <TEXTURE id="null" />

      <SCALE sx="1" sy="0.3" sz="1" />

      <DESCENDANTS>
        <NODEREF id="box" />
      </DESCENDANTS>
    </NODE>

    <NODE id="wheels">
      <MATERIAL id="null" />
      <TEXTURE id="null" />

      <TRANSLATION x="0" y="-0.3" z="0" />

      <DESCENDANTS>
        <NODEREF id="sup1" />
        <NODEREF id="sup2" />
      </DESCENDANTS>
    </NODE>

    <NODE id="sup1">
      <MATERIAL id="null" />
      <TEXTURE id="null" />

      <DESCENDANTS>
        <NODEREF id="cyl1" />
      </DESCENDANTS>
    </NODE>

    <NODE id="cyl1">
      <MATERIAL id="null" />
      <TEXTURE id="null" />

      <TRANSLATION x="0" y="0.3" z="0.5" />

      <DESCENDANTS>
        <NODEREF id="cyl2" />
