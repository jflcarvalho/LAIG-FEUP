<SCENE>

<!-- Initial values -->
<INITIALS>
    <frustum near="0.001" far="500"/>                 
    <translation x="-1" y="1" z="0" />               
    <rotation axis="x" angle="0" />                
    <rotation axis="y" angle="0" />                
    <rotation axis="z" angle="0" />                
    <scale sx="0.25" sy="0.25" sz="0.25" />         
    <reference length="5" />                        
</INITIALS>


<!-- Illumination and clear colour -->
<ILLUMINATION>
    <ambient r="1" g="1" b="1" a="1" />       
    <background r="0" g="0" b="0" a="1" />          
</ILLUMINATION>

<LIGHTS>	
	<LIGHT id="l_sun">                                    
        <enable value ="1" />                   	       
        <position  x="0" y="0" z="0" w="1" />       	   
        <ambient r="0.6" g="0.6" b="0.6" a="1.0" />        	 
        <diffuse r="1.0" g="1.0" b="1.0" a="1.0" />        
        <specular r="1.0" g="1.0" b="1.0" a="1.0" />      
    </LIGHT>

    <!-- DEBUG LIGHTS -->	
	<LIGHT id="light1">                                    
        <enable value ="1" />                   	       
        <position  x="100" y="100" z="100" w="1" />       	   
        <ambient r="0.2" g="0.2" b="0.2" a="1.0" />        	 
        <diffuse r="0.6" g="0.6" b="0.6" a="1.0" />         
        <specular r="0.2" g="0.2" b="0.2" a="1.0" />     
    </LIGHT>	
	<LIGHT id="light2">                                    
        <enable value ="1" />                   	       
        <position  x="-100" y="100" z="100" w="1" />    	   
        <ambient r="0.2" g="0.2" b="0.2" a="1.0" />        	 
        <diffuse r="0.6" g="0.6" b="0.6" a="1.0" />         
        <specular r="0.2" g="0.2" b="0.2" a="1.0" />       
    </LIGHT>	
	<LIGHT id="light3">                                    
        <enable value ="1" />                   	       
        <position  x="100" y="100" z="-100" w="1" />    	   
        <ambient r="0.2" g="0.2" b="0.2" a="1.0" />        	 
        <diffuse r="0.6" g="0.6" b="0.6" a="1.0" />         
        <specular r="0.2" g="0.2" b="0.2" a="1.0" />       
    </LIGHT>	
	<LIGHT id="light4">                                    
        <enable value ="1" />                   	       
        <position  x="-100" y="100" z="-100" w="1" />  	   
        <ambient r="0.2" g="0.2" b="0.2" a="1.0" />        	 
        <diffuse r="0.6" g="0.6" b="0.6" a="1.0" />         
        <specular r="0.2" g="0.2" b="0.2" a="1.0" />          
    </LIGHT>
</LIGHTS>


<!-- Textures -->
<TEXTURES> 
	<!-- Tree trunk -->
    <TEXTURE id="t_sun">
        <file path="images/trunk-image.jpg" />               
        <amplif_factor s="2" t="2" />                           
    </TEXTURE>
</TEXTURES>


<MATERIALS>
    <!-- A default material -->
    <MATERIAL id="defaultMaterial">                                  
        <shininess value="1" />
        <specular r="0.0" g="0.0" b="0.0" a="1" />        
        <diffuse r="0.4" g="0.4" b="0.4" a="1" />       
        <ambient r="0.4" g="0.4" b="0.4" a="1" />         
        <emission r="0.0" g="0.0" b="0.0" a="1" />        
    </MATERIAL>

    
    <!-- A default material -->
    <MATERIAL id="m_sun">                                  
        <shininess value="1" />
        <specular r="0.0" g="0.0" b="0.0" a="1" />        
        <diffuse r="0" g="0" b="0" a="1" />       
        <ambient r="0.2" g="0.2" b="0.2" a="1" />         
        <emission r="1" g="1" b="0.2" a="1" />        
    </MATERIAL>

</MATERIALS>


<!-- The leaves -->
<LEAVES>
</LEAVES>


<!-- The nodes -->
<NODES>

    <ROOT id="root" />     

    <!-- The root node -->
    <NODE id="root">       
        <MATERIAL id="defaultMaterial" />         
        <TEXTURE id="null" />  


        <DESCENDANTS>
            <NODEREF id="robot" />
        </DESCENDANTS>

    </NODE>

    <!-- ROBOT -->
    <NODE id="robot">      
        <MATERIAL id="null" />    
        <TEXTURE id="null" />

        <SCALE sx="3" sy="3" sz="3" />
        <TRANSLATION x="0" y="0" z="0" />
        
        <DESCENDANTS>
            <NODEREF id="body" />  
            <NODEREF id="camera" />   
            <NODEREF id="wheels" />     
		</DESCENDANTS>
    </NODE>

    <NODE id="camera">      
        <MATERIAL id="null" />    
        <TEXTURE id="null" />

		<ROTATION axis="x" angle="-90" />

        <DESCENDANTS>
            <NODEREF id="camera1" /> 
            <NODEREF id="camera2" /> 
            <LEAF id="p_cyl1" type="cylinder" args="1 0.05 0.05 20 20" />
		</DESCENDANTS>
    </NODE>

    <NODE id="camera1">      
        <MATERIAL id="null" />    
        <TEXTURE id="null" />

        <TRANSLATION x="0" y="0" z="1" />
        <SCALE sx="0.1" sy="0.4" sz="0.25" />

        <DESCENDANTS>
            <NODEREF id="box" /> 
		</DESCENDANTS>
    </NODE>

    <NODE id="camera2">      
        <MATERIAL id="null" />    
        <TEXTURE id="null" />

        <TRANSLATION x="0" y="0" z="1" />
		<ROTATION axis="y" angle="90" />

        <DESCENDANTS>
            <LEAF id="p_cyl1" type="cylinder" args="0.1 0.05 0.05 20 20" />
		</DESCENDANTS>
    </NODE>

    <NODE id="body">      
        <MATERIAL id="null" />    
        <TEXTURE id="null" />

        <SCALE sx="1" sy="0.3" sz="1" />

        <DESCENDANTS>  
            <NODEREF id="box" />  
		</DESCENDANTS>
    </NODE>

    <!-- WHEELS -->
    <NODE id="wheels">      
        <MATERIAL id="null" />    
        <TEXTURE id="null" />

        <TRANSLATION x="0" y="-0.3" z="0" />

        <DESCENDANTS>
            <NODEREF id="sup1" />
            <NODEREF id="sup2" />
		</DESCENDANTS>
    </NODE>

    <NODE id="sup1">      
        <MATERIAL id="null" />    
        <TEXTURE id="null" />

        <DESCENDANTS>  
            <NODEREF id="cyl1" />
		</DESCENDANTS>
    </NODE>

    <NODE id="sup2">      
        <MATERIAL id="null" />    
        <TEXTURE id="null" />

        <SCALE sx="1" sy="1" sz="-1" />

        <DESCENDANTS>  
            <NODEREF id="cyl1" />
		</DESCENDANTS>
    </NODE>

    <NODE id="cyl1">      
        <MATERIAL id="null" />    
        <TEXTURE id="null" />

        <TRANSLATION x="0" y="0.3" z="0.5" />

        <DESCENDANTS>
            <NODEREF id="cyl2" />
            <NODEREF id="cyl4" />
            <LEAF id="p_cyl1" type="cylinder" args="0.3 0.05 0.05 20 20" />
		</DESCENDANTS>
    </NODE>

    <NODE id="cyl2">      
        <MATERIAL id="null" />    
        <TEXTURE id="null" />

        <TRANSLATION x="0" y="0" z="0.2" />
		<ROTATION axis="y" angle="90" />
		<ROTATION axis="x" angle="30" />

        <DESCENDANTS>
            <NODEREF id="cyl3" />
            <LEAF id="p_cyl2" type="cylinder" args="0.6 0.05 0.05 20 20" />
		</DESCENDANTS>
    </NODE>

    <NODE id="cyl3">      
        <MATERIAL id="null" />    
        <TEXTURE id="null" />

		<ROTATION axis="x" angle="-30" />
		<ROTATION axis="y" angle="-90" />
        <TRANSLATION x="0.52" y="-0.3" z="-0.05" />

        <DESCENDANTS>
            <NODEREF id="wheel1" />
            <LEAF id="p_cyl3" type="cylinder" args="0.3 0.05 0.05 20 20" />
		</DESCENDANTS>
    </NODE>

    <NODE id="cyl4">      
        <MATERIAL id="null" />    
        <TEXTURE id="null" />

        <TRANSLATION x="0" y="0" z="0.1" />
		<ROTATION axis="y" angle="90" />
		<ROTATION axis="x" angle="150" />

        <DESCENDANTS>
            <NODEREF id="cyl5" />
            <LEAF id="p_cyl4" type="cylinder" args="0.6 0.05 0.05 20 20" />
		</DESCENDANTS>
    </NODE>

    <NODE id="cyl5">      
        <MATERIAL id="null" />    
        <TEXTURE id="null" />

		<ROTATION axis="x" angle="-150" />
		<ROTATION axis="y" angle="-90" />
        <TRANSLATION x="-0.52" y="-0.3" z="-0.05" />

        <DESCENDANTS>
            <NODEREF id="cyl6" />
            <LEAF id="p_cyl5" type="cylinder" args="0.2 0.05 0.05 20 20" />
		</DESCENDANTS>
    </NODE>

    <NODE id="cyl6">      
        <MATERIAL id="null" />    
        <TEXTURE id="null" />

        <TRANSLATION x="-0.3" y="0" z="0.2" />
		<ROTATION axis="y" angle="90" />

        <DESCENDANTS>
            <NODEREF id="cyl7" />
            <NODEREF id="cyl8" />
            <LEAF id="p_cyl6" type="cylinder" args="0.6 0.05 0.05 20 20" />
		</DESCENDANTS>
    </NODE>

    <NODE id="cyl7">      
        <MATERIAL id="null" />    
        <TEXTURE id="null" />
        
		<ROTATION axis="y" angle="-90" />
        <TRANSLATION x="0" y="0" z="-0.05" />

        <DESCENDANTS>
            <NODEREF id="wheel2" />
            <LEAF id="p_cyl7" type="cylinder" args="0.25 0.05 0.05 20 20" />
		</DESCENDANTS>
    </NODE>

    <NODE id="cyl8">      
        <MATERIAL id="null" />    
        <TEXTURE id="null" />

		<ROTATION axis="y" angle="-90" />
        <TRANSLATION x="0.6" y="0" z="-0.05" />

        <DESCENDANTS>
            <NODEREF id="wheel3" />
            <LEAF id="p_cyl8" type="cylinder" args="0.25 0.05 0.05 20 20" />
		</DESCENDANTS>
    </NODE>

    <NODE id="wheel1">      
        <MATERIAL id="null" />    
        <TEXTURE id="null" />

        <TRANSLATION x="0" y="0" z="0.3" />

        <DESCENDANTS>
            <LEAF id="p_wheel1" type="cylinder" args="0.2 0.2 0.2 20 20" /> 
		</DESCENDANTS>
    </NODE>

    <NODE id="wheel2">      
        <MATERIAL id="null" />    
        <TEXTURE id="null" />

        <TRANSLATION x="0" y="0" z="0.25" />

        <DESCENDANTS>
            <LEAF id="p_wheel2" type="cylinder" args="0.2 0.2 0.2 20 20" /> 
		</DESCENDANTS>
    </NODE>

    <NODE id="wheel3">      
        <MATERIAL id="null" />    
        <TEXTURE id="null" />

        <TRANSLATION x="0" y="0" z="0.25" />

        <DESCENDANTS>
            <LEAF id="p_wheel3" type="cylinder" args="0.2 0.2 0.2 20 20" /> 
		</DESCENDANTS>
    </NODE>

    <!-- UTILS -->
    <NODE id="box">      
        <MATERIAL id="null" />    
        <TEXTURE id="null" />

        <DESCENDANTS>
            <NODEREF id="face1" />
            <NODEREF id="face2" />
            <NODEREF id="face3" />
            <NODEREF id="face4" />
            <NODEREF id="face5" />
            <NODEREF id="face6" />
        </DESCENDANTS>
    </NODE>

    <!-- A cube face -->
    <NODE id="face1">       
        <MATERIAL id="null" />     
        <TEXTURE id="null" />       			

        <TRANSLATION x="0" y="0" z="0.5" />

        <DESCENDANTS>
					<LEAF  type="rectangle" args="-0.5 0.5 0.5 -0.5" />        
        </DESCENDANTS>
    </NODE>

    <NODE id="face2">       
        <MATERIAL id="null" />     
        <TEXTURE id="null" />       			

		<ROTATION axis="y" angle="-90" />
        <TRANSLATION x="0" y="0" z="0.5" />

        <DESCENDANTS>
					<LEAF  type="rectangle" args="-0.5 0.5 0.5 -0.5" />        
        </DESCENDANTS>
    </NODE>

    <NODE id="face3">       
        <MATERIAL id="null" />     
        <TEXTURE id="null" />       			

		<ROTATION axis="y" angle="-180" />
        <TRANSLATION x="0" y="0" z="0.5" />

        <DESCENDANTS>
					<LEAF  type="rectangle" args="-0.5 0.5 0.5 -0.5" />        
        </DESCENDANTS>
    </NODE>

    <NODE id="face4">       
        <MATERIAL id="null" />     
        <TEXTURE id="null" />       			

		<ROTATION axis="y" angle="90" />
        <TRANSLATION x="0" y="0" z="0.5" />

        <DESCENDANTS>
					<LEAF  type="rectangle" args="-0.5 0.5 0.5 -0.5" />        
        </DESCENDANTS>
    </NODE>

    <NODE id="face5">       
        <MATERIAL id="null" />     
        <TEXTURE id="null" />       			

		<ROTATION axis="x" angle="-90" />
        <TRANSLATION x="0" y="0" z="0.5" />

        <DESCENDANTS>
					<LEAF  type="rectangle" args="-0.5 0.5 0.5 -0.5" />        
        </DESCENDANTS>
    </NODE>
    
    <NODE id="face6">       
        <MATERIAL id="null" />     
        <TEXTURE id="null" />       			

		<ROTATION axis="x" angle="90" />
        <TRANSLATION x="0" y="0" z="0.5" />

        <DESCENDANTS>
					<LEAF  type="rectangle" args="-0.5 0.5 0.5 -0.5" />        
        </DESCENDANTS>
    </NODE>
</NODES>

</SCENE>