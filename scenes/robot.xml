<SCENE>

<!-- Initial values -->
<INITIALS>
    <frustum near="0" far="500"/>                 
    <translation x="0" y="0" z="0" />               
    <rotation axis="x" angle="0" />                
    <rotation axis="y" angle="0" />                
    <rotation axis="z" angle="0" />                
    <scale sx="0.25" sy="0.25" sz="0.25" />         
    <reference length="5" />                        
</INITIALS>


<!-- Illumination and clear colour -->
<ILLUMINATION>
    <ambient r="0.25" g="0.25" b="0.25" a="1" />       
    <background r="0" g="0" b="0" a="1" />          
</ILLUMINATION>

<LIGHTS>
  <LIGHT id="light0">
    <enable value ="1" />
    <position  x="0" y="10" z="0" w="1" />
    <ambient r="0.2" g="0.2" b="0.2" a="1.0" />
    <diffuse r="1.0" g="1.0" b="1.0" a="1.0" />
    <specular r="1.0" g="1.0" b="1.0" a="1.0" />
  </LIGHT>

  <LIGHT id="light1">
    <enable value ="1" />
    <position x="30" y="10" z="0" w="1" />
    <ambient r="0.2" g="0.2" b="0.2" a="1.0" />
    <diffuse r="1.0" g="1.0" b="1.0" a="1.0" />
    <specular r="1.0" g="1.0" b="1.0" a="1.0" />
  </LIGHT>

  <LIGHT id="light2">
    <enable value ="1" />
    <position x="0" y="10" z="20" w="1" />
    <ambient r="0.2" g="0.2" b="0.2" a="1.0" />
    <diffuse r="1.0" g="1.0" b="1.0" a="1.0" />
    <specular r="1.0" g="1.0" b="1.0" a="1.0" />
  </LIGHT>

  <LIGHT id="light3">
    <enable value ="1" />
    <position x="30" y="10" z="20" w="1" />
    <ambient r="0.2" g="0.2" b="0.2" a="1.0" />
    <diffuse r="0.6" g="0.6" b="0.6" a="1.0" />
    <specular r="0.6" g="0.6" b="0.6" a="1.0" />
  </LIGHT>

  <LIGHT id="light4">
    <enable value ="1" />
    <position x="0" y="-10" z="0" w="1" />
    <ambient r="0.2" g="0.2" b="0.2" a="1.0" />
    <diffuse r="0.6" g="0.6" b="0.6" a="1.0" />
    <specular r="0.6" g="0.6" b="0.6" a="1.0" />
  </LIGHT>
</LIGHTS>


<TEXTURES> 
    <TEXTURE id="t_rubber">
        <file path="images/misc/tyre_rubber.jpg" />
        <amplif_factor s="1" t="1" />
    </TEXTURE>

    <TEXTURE id="t_aluminium">
      <file path="images/misc/aluminium.jpg" />
      <amplif_factor s="2" t="1" />
    </TEXTURE>
</TEXTURES>


<MATERIALS>
  <MATERIAL id="m_default">
    <shininess value="10" />
    <specular r="0.5" g="0.5" b="0.5" a="1" />
    <diffuse r="0.5" g="0.5" b="0.5" a="1" />
    <ambient r="0.1" g="0.1" b="0.1" a="1" />
    <emission r="0.0" g="0.0" b="0.0" a="1" />
  </MATERIAL>

  <MATERIAL id="m_robot_steel">
    <shininess value="64" />
    <specular r="0.85" g="0.85" b="0.85" a="1" />
    <diffuse r="0.25" g="0.25" b="0.25" a="1" />
    <ambient r="0.1" g="0.1" b="0.1" a="1" />
    <emission r="0" g="0" b="0" a="1" />
  </MATERIAL>

  <MATERIAL id="m_robot_wheel">
    <shininess value="1" />
    <specular r="0.2" g="0.2" b="0.2" a="1" />
    <diffuse r="0.6" g="0.6" b="0.6" a="1" />
    <ambient r="0.1" g="0.1" b="0.1" a="1" />
    <emission r="0" g="0" b="0" a="1" />
  </MATERIAL>
</MATERIALS>


<!-- The leaves -->
<LEAVES>
</LEAVES>


<!-- The nodes -->
<NODES>

    <ROOT id="root" />     

    <!-- The root node -->
    <NODE id="root">       
        <MATERIAL id="m_default" />         
        <TEXTURE id="null" />  
        <DESCENDANTS>
            <NODEREF id="robot" />
        </DESCENDANTS>

    </NODE>

  <!-- Robot -->
  <NODE id="robot">
    <MATERIAL id="m_robot_steel" />
    <TEXTURE id="t_aluminium" />

    <DESCENDANTS>
      <NODEREF id="body" />
      <NODEREF id="camera" />
      <NODEREF id="wheels" />
    </DESCENDANTS>
  </NODE>

  <NODE id="camera">
    <MATERIAL id="null" />
    <TEXTURE id="null" />

    <ROTATION axis="x" angle="-90" />

    <DESCENDANTS>
      <NODEREF id="camera1" />
      <NODEREF id="camera2" />
      <LEAF id="p_cyl1" type="cylinder" args="1 0.05 0.05 20 20 1 1" />
    </DESCENDANTS>
  </NODE>

  <NODE id="camera1">
    <MATERIAL id="null" />
    <TEXTURE id="null" />

    <TRANSLATION x="0" y="0" z="1" />
    <SCALE sx="0.1" sy="0.4" sz="0.25" />

    <DESCENDANTS>
      <NODEREF id="box" />
    </DESCENDANTS>
  </NODE>

  <NODE id="camera2">
    <MATERIAL id="null" />
    <TEXTURE id="null" />

    <TRANSLATION x="0" y="0" z="1" />
    <ROTATION axis="y" angle="90" />

    <DESCENDANTS>
      <LEAF id="p_cyl1" type="cylinder" args="0.1 0.05 0.05 20 20 1 1" />
    </DESCENDANTS>
  </NODE>

  <NODE id="body">
    <MATERIAL id="null" />
    <TEXTURE id="null" />

    <SCALE sx="1" sy="0.3" sz="1" />

    <DESCENDANTS>
      <NODEREF id="box" />
    </DESCENDANTS>
  </NODE>

  <NODE id="wheels">
    <MATERIAL id="null" />
    <TEXTURE id="null" />

    <TRANSLATION x="0" y="-0.3" z="0" />

    <DESCENDANTS>
      <NODEREF id="sup1" />
      <NODEREF id="sup2" />
    </DESCENDANTS>
  </NODE>

  <NODE id="sup1">
    <MATERIAL id="null" />
    <TEXTURE id="null" />

    <DESCENDANTS>
      <NODEREF id="cyl1" />
    </DESCENDANTS>
