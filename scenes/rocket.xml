          </CPLINE>
          <CPLINE>
              <CPOINT xx="0" yy="10" zz="0" ww="1.0" />
              <CPOINT xx="0.3535" yy="10" zz="0.3535" ww="2.0" />
              <CPOINT xx="1" yy="7" zz="2" ww="10.0" />
              <CPOINT xx="1" yy="0" zz="2" ww="20.0" />
              <CPOINT xx="0" yy="0" zz="0" ww="1.0" />
          </CPLINE>
          <CPLINE>
              <CPOINT xx="0" yy="10" zz="0" ww="1.0" />
              <CPOINT xx="0.3535" yy="10" zz="0.3535" ww="2.0" />
              <CPOINT xx="1" yy="7" zz="2" ww="10.0" />
