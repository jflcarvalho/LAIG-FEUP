          </CPLINE>
          <CPLINE>
              <CPOINT xx="0" yy="10" zz="0" ww="1.0" />
              <CPOINT xx="-0.5" yy="10" zz="0" ww="2.0" />
              <CPOINT xx="-1" yy="7" zz="0" ww="10.0" />
              <CPOINT xx="-1" yy="0" zz="0" ww="20.0" />
              <CPOINT xx="0" yy="0" zz="0" ww="1.0" />
          </CPLINE>
          <CPLINE>
              <CPOINT xx="0" yy="10" zz="0" ww="1.0" />
              <CPOINT xx="-0.5" yy="10" zz="0" ww="2.0" />
              <CPOINT xx="-1" yy="7" zz="0" ww="10.0" />
              <CPOINT xx="-1" yy="0" zz="0" ww="20.0" />
              <CPOINT xx="0" yy="0" zz="0" ww="1.0" />
          </CPLINE>
          <CPLINE>
              <CPOINT xx="0" yy="10" zz="0" ww="1.0" />
              <CPOINT xx="0" yy="10" zz="0" ww="1.0" />
              <CPOINT xx="0" yy="7" zz="0" ww="1.0" />
              <CPOINT xx="0" yy="0" zz="0" ww="1.0" />
              <CPOINT xx="0" yy="0" zz="0" ww="1.0" />
          </CPLINE>
        </LEAF>
      </DESCENDANTS>
    </NODE>

    <NODE id="spaceship_wings">
      <MATERIAL id="null" />
      <TEXTURE id="null" />
      <DESCENDANTS>
        <LEAF type="patch" args="50 50"> 
          <CPLINE>
