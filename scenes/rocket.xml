          </CPLINE>
          <CPLINE>
            <CPOINT xx="-4" yy="0" zz="0" ww="1.0" />
            <CPOINT xx="-4" yy="2" zz="0" ww="2.0" />
            <CPOINT xx="-1" yy="2" zz="0" ww="2.0" />
            <CPOINT xx="-1.5" yy="8" zz="0" ww="2.0" />
            <CPOINT xx="0" yy="9.5" zz="0" ww="3.0" />
          </CPLINE>
          <CPLINE>
